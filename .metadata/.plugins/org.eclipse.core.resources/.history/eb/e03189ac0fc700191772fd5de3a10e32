package com.BasicJava;

public class Arraysingledimension {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int array1[] = new int[3];
      array1[0] =10;
      array1[1]=20;
       array1[2]=30;


for (int i=0; i<3; i++)
{
	
	System.out.println(array1[i]);
}

}
	}


